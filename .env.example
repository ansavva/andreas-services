# Environment configuration for the Gmail ingestion container
TABLE_NAME=Events
TIMEZONE=America/New_York
OPENAI_MODEL=gpt-4o-mini
AWS_REGION=us-east-1
GMAIL_QUERY=label:Events

# Local-only credentials (do not commit real values)
OPENAI_API_KEY=sk-your-openai-key
GMAIL_CREDENTIALS_FILE=./gmail-credentials.json
AWS_ACCESS_KEY_ID=fake
AWS_SECRET_ACCESS_KEY=fake

# Optional overrides for local development (e.g., DynamoDB Local)
DYNAMODB_ENDPOINT_URL=http://localhost:8000
COMPOSE_DYNAMODB_ENDPOINT_URL=http://dynamodb:8000
