# Local development environment variables
# Copy this file to .env.local and fill in your values
#
# IMPORTANT: After deploying dev infrastructure with Terraform, run:
#   cd storybook/infra
#   terraform apply -var-file=terraform.dev.tfvars
#   terraform output
# Then copy the cognito outputs below

VITE_API_URL=http://localhost:5001
VITE_AWS_COGNITO_USER_POOL_ID=your_dev_user_pool_id_from_terraform_output
VITE_AWS_COGNITO_APP_CLIENT_ID=your_dev_app_client_id_from_terraform_output
VITE_AWS_COGNITO_DOMAIN=your-dev-cognito-domain.auth.us-east-1.amazoncognito.com
