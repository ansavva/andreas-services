        "WriteTo": [
            {
                "Name": "MongoDB",
                "Args": {
                    "databaseUrl": "mongodb+srv://admin:bwrb9ranUw6dvsU5@cluster0-8knw2.mongodb.net/HumbuggDb?retryWrites=true&w=majority",
                    "collectionName": "Log"
                }
            }
        ]
    },
    "AllowedHosts": "*",
